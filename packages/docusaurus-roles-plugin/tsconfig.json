{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "types": [
      "@docusaurus/module-type-aliases",
      "@docusaurus/theme-classic"
    ]
  },
  "include": [
    "src"
  ]
}
